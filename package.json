{
  "name": "postcss-meter",
  "version": "1.0.0",
  "description": "Cross-browser meter pseudo-selectors",
  "homepage": "https://github.com/Myhlamaeus/postcss-meter",
  "author": {
    "name": "Malte-Maurice Dreyer",
    "email": "dreyer.maltem+dev@gmail.com",
    "url": "https://github.com/Myhlamaeus"
  },
  "files": ["lib"],
  "main": "lib/index",
  "keywords": [
    "postcss",
    "meter",
    "cross",
    "browser",
    "pseudo",
    "class",
    "element",
    "selector"
  ],
  "devDependencies": {
    "clean-css": "^4.1.9",
    "coveralls": "^2.13.1",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "git@github.com:Myhlamaeus/postcss-meter.git",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "@std/esm": "^0.18.0",
    "postcss": "^6.0.14"
  }
}
